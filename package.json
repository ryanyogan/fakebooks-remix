{
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "run-s \"build:*\"",
    "build:css": "npm run generate:css -- --minify",
    "build:remix": "remix build",
    "dev": "run-p \"dev:*\"",
    "dev:css": "npm run generate:css -- --watch",
    "dev:remix": "remix dev",
    "generate:css": "npx tailwindcss -o ./app/styles/tailwind.css",
    "start": "remix-serve build"
  },
  "dependencies": {
    "@prisma/client": "^4.1.0",
    "@reach/dialog": "^0.17.0",
    "@remix-run/node": "^1.6.5",
    "@remix-run/react": "^1.6.5",
    "@remix-run/serve": "^1.6.5",
    "bcryptjs": "^2.4.3",
    "clsx": "^1.2.1",
    "downshift": "^6.1.7",
    "isbot": "^3.5.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "spin-delay": "^1.2.0",
    "tiny-invariant": "^1.2.0"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.6.5",
    "@remix-run/eslint-config": "^1.6.5",
    "@types/bcryptjs": "^2.4.2",
    "@types/eslint": "^8.4.5",
    "@types/node": "^18.0.6",
    "@types/react": "^17.0.45",
    "@types/react-dom": "^17.0.17",
    "autoprefixer": "^10.4.7",
    "eslint": "^8.15.0",
    "eslint-config-prettier": "^8.5.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.14",
    "prettier": "^2.7.1",
    "prisma": "^4.1.0",
    "tailwindcss": "^3.1.6",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.0.0",
    "typescript": "^4.7.4"
  },
  "engines": {
    "node": ">=14"
  },
  "prisma": {
    "seed": "ts-node --require tsconfig-paths/register prisma/seed.ts"
  }
}
